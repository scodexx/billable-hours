{
  "name": "billable-hours",
  "version": "0.1.0",
  "description": "",
  "keywords": [
    "boilerplate",
    "billable",
    "coverage",
    "testing"
  ],
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=11.7.0",
    "npm": ">=6.10.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scodexx/billable-hours.git"
  },
  "author": {
    "name": "scodexx",
    "email": "hello@sheyi.me"
  },
  "dependencies": {
    "@bugsnag/js": "6.3.2",
    "@bugsnag/plugin-express": "6.3.0",
    "@bugsnag/plugin-react": "6.2.0",
    "axios": "0.19.0",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "csvtojson": "2.0.10",
    "dotenv": "8.0.0",
    "errorhandler": "1.5.1",
    "express": "4.17.1",
    "express-fileupload": "1.1.5",
    "express-limiter": "1.6.1",
    "helmet": "3.20.0",
    "ignore-styles": "5.0.1",
    "lodash": "4.17.15",
    "method-override": "3.0.0",
    "moment": "2.24.0",
    "morgan": "1.9.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-helmet-async": "1.0.2",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "redis": "2.8.0",
    "sweetalert2": "8.14.0",
    "sweetalert2-react-content": "1.1.0",
    "typescript": "3.5.3",
    "uniqid": "5.0.3"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "7.5.5",
    "autoprefixer": "^9.6.1",
    "babel-plugin-istanbul": "5.2.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chai": "4.2.0",
    "cross-env": "5.2.0",
    "cssnano": "^4.1.10",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint-plugin-react": "7.14.3",
    "ghooks": "2.0.4",
    "gulp": "4.0.2",
    "gulp-postcss": "^8.0.0",
    "gulp-util": "^3.0.8",
    "istanbul": "0.4.5",
    "mocha": "6.2.0",
    "nodemon": "1.19.1",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "postcss-cli": "6.1.3",
    "postcss-color-function": "^4.1.0",
    "postcss-nested": "4.1.2",
    "precss": "^4.0.0",
    "react-to-print": "2.3.0",
    "rimraf": "2.6.3",
    "sinon": "7.3.2",
    "snyk": "1.205.0",
    "stylelint": "10.1.0",
    "tslint": "5.18.0"
  },
  "scripts": {
    "start": "npm-run-all --parallel \"start:web\" \"watch:server\" \"postcss:watch\"",
    "start:web": "react-scripts start",
    "start:server": "node server/index.js",
    "watch:server": "nodemon --watch 'server/**/*' server/index.js",
    "test": "react-scripts test --env=jsdom",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test:snyk": "snyk test",
    "test:backend": "cross-env NODE_ENV=development nyc mocha --exit --recursive \"server/tests/**/*.test.js\"",
    "test:frontend": "cross-env NODE_ENV=development nyc mocha --exit --recursive \"src/tests/**/*.test.js\"",
    "coverage:check": "nyc check-coverage",
    "coverage:report": "nyc report --reporter=lcov --reporter=text",
    "lint": "run-s -s lint:*",
    "lint:backend": "eslint --ignore-path .eslintignore 'server/**/*.js'",
    "lint:frontend": "eslint --ignore-path .eslintignore 'src/**/*.js'",
    "lint:css": "stylelint public/**/*.css",
    "postcss:build": "postcss public/static/css/_style.css -o public/static/css/style.css",
    "postcss:watch": "postcss public/static/css/_style.css -o public/static/css/style.css -w",
    "clean": "rimraf ./.nyc_output ./node_modules/.cache",
    "build": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:frontend && npm run check-coverage"
    }
  },
  "nyc": {
    "branches": 50,
    "lines": 50,
    "functions": 50,
    "statements": 50,
    "require": [
      "babel-polyfill",
      "@babel/register"
    ],
    "extensions": [
      ".js"
    ],
    "exclude": [
      "coverage/**",
      "server/tests/**",
      "src/tests/**",
      "mode_modules/**",
      "build/**",
      ".nyc_output/**"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "sourceMap": false,
    "cache": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
